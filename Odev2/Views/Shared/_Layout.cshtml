<!DOCTYPE html>
<html lang="en">
<!-- Mirrored from ndon.pl/themes/ZUPA/2.0/card.html by HTTrack Website Copier/3.x [XR&CO'2014], Thu, 29 Nov 2018 18:31:54 GMT -->
<head>
    <title>ZUPA KAFE</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!--[if lt IE 9]>
    <script src="//html5shiv.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
    <link rel="shortcut icon" href="../Content/images/favicon.html">
    <link href='https://fonts.googleapis.com/css?family=Lato:400,300,700&amp;subset=latin,latin-ext' rel='stylesheet' type='text/css'>
    <link href="https://fonts.googleapis.com/css?family=Playfair+Display+SC:400,700,900&amp;subset=latin-ext" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="../Content/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="../Content/css/animate.css">
    <link rel="stylesheet" type="text/css" href="../Content/css/fontawesome-all.min.css">
    <link rel="stylesheet" type="text/css" href="../Content/css/swiper.min.css">
    <link rel="stylesheet" type="text/css" href="../Content/css/style.css">


</head>
<body>

    <header>
        <div class="container d-none d-sm-block logo">
            <a href="index.html"><img class="img-fluid" src="../Content/images/logos/logo.png" alt="Logo"></a>
        </div>
        <div class="container my-2 my-md-4">
            <nav class="navbar navbar-expand-sm navbar-light">

                <div class="collapse navbar-collapse justify-content-sm-center" id="main-navbar">
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            @Html.ActionLink("Menü", "Index", "Home", new { @class = "nav-link" })
                            @*<a class="nav-link" href="Home/Index">Menu</a>*@
                        </li>
                        <li class="nav-item">
                            @*<a class="nav-link" href="Home/Contact">İletişim</a>*@
                            @Html.ActionLink("İletişim", "Contact", "Home", new { @class = "nav-link" })
                        </li>

                        @if (Session["KULLANICIADI"] != null)
                        {

                            <li class="nav-item dropdown">
                                <a class="nav-link" href="#" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">@Session["KULLANICIADI"] <span class="dropdown-toggle"></span></a>
                                <ul class="dropdown-menu">
                                    <li>
                                        @Html.ActionLink("Çıkış", "Logout", "Home", new { @class = "nav-link" })
                                    </li>
                                </ul>
                            </li>
                        }
                        else
                        {
                            <li class="nav-item">
                                @Html.ActionLink("Giriş", "Giris", "Home", new { @class = "nav-link" })
                            </li>
                        }
                        @if (Session["KULLANICIRANK"]=="admin")
                        {
                            <li class="nav-item dropdown">
                                <a class="nav-link" href="#" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">İşlemler <span class="dropdown-toggle"></span></a>
                                <ul class="dropdown-menu">
                                    <li>
                                        @Html.ActionLink("Ürün Ekle", "UrunEkle", "Home", new { @class = "nav-link" })
                                    </li>
                                </ul>
                            </li>
                        }
                        </ul>
                </div>
            </nav>
        </div>
    </header>

    @RenderBody()
    <footer class="page-footer">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <div class="divider"></div>
                    <div class="text-center">
                        <a href="index.html"><img src="../images/logos/logo.png" alt="" class="logo"></a>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-9">
                    <ul class="footer-links">
                        <li>
                            <a href="#">Privacy Policy</a>
                        </li>
                        <li>
                            <a href="blog-index.html">Blog</a>
                        </li>
                        <li>
                            <a href="contact.html">Contact</a>
                        </li>
                    </ul>
                </div>
                <div class="col-sm-3">
                    <p>© ZUPA 2018</p>
                </div>
            </div>
        </div>
    </footer>
    <script src="https://code.jquery.com/jquery-2.2.4.min.js"></script>
    <script src="../Content/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="../Content/javascript/acos.jquery.js"></script>
    <script src="../Content/javascript/script.js"></script>
</body>
